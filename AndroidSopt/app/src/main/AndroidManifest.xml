<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidsopt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidSopt"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.Profile.SettingActivity"></activity>
        <activity android:name=".view.Login.SignInActivity" />
        <activity android:name=".ExampleActivity" />
        <activity android:name=".view.Home.HomeActivity" />
        <activity android:name=".view.onBoarding.OnBoarding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- android:hardwareAccelerated="false" <activity android:name=".SignUpActivity"></activity> -->
        <!-- <activity android:name=".SignInActivity"> -->
        <activity android:name=".view.Login.SignUpActivity" />
    </application>

</manifest>